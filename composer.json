  "type": "craft-plugin",
  "version": "3.0.0-beta.5",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "currency prices"
  ],
  "support": {
    "docs": "https://github.com/webdna/commerce-currency-prices",
    "issues": "https://github.com/webdna/commerce-currency-prices/issues?state=open"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "webdna",
      "homepage": "https://webdna.co.uk"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0",
    "craftcms/commerce": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "webdna\\commerce\\currencyprices\\": "src/"
    }
  },
  "extra": {
